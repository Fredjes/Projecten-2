<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TabPane tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <tabs>
    <Tab text="Details CD">
         <content>
            <HBox alignment="CENTER_LEFT" spacing="10.0">
               <children>
                  <GridPane hgap="10.0" vgap="8.0">
                    <columnConstraints>
                      <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="-Infinity" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="-Infinity" />
                        <ColumnConstraints hgrow="SOMETIMES" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints valignment="CENTER" vgrow="SOMETIMES" />
                      <RowConstraints valignment="TOP" vgrow="SOMETIMES" />
                      <RowConstraints valignment="CENTER" vgrow="SOMETIMES" />
                        <RowConstraints valignment="CENTER" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Titel:" />
                        <Label text="Omschrijving:" GridPane.rowIndex="1" />
                        <Label text="Thema's:" GridPane.rowIndex="2" />
                        <TextField maxWidth="-Infinity" prefWidth="200.0" GridPane.columnIndex="1" />
                        <TextArea maxHeight="-Infinity" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField maxWidth="-Infinity" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                     </children>
                  </GridPane>
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <GridPane hgap="10.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="-Infinity" />
                      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="-Infinity" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Label text="Leeftijdcategorie:" />
                        <TextField maxHeight="-Infinity" maxWidth="-Infinity" prefWidth="150.0" GridPane.columnIndex="1" />
                        <Label text="Artiest:" GridPane.rowIndex="1" />
                        <TextField maxWidth="-Infinity" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     </children>
                  </GridPane>
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <VBox alignment="CENTER" spacing="10.0">
                     <children>
                        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Button mnemonicParsing="false" text="Afbeelding veranderen">
                           <styleClass>
                              <String fx:value="btn" />
                              <String fx:value="btn-lime" />
                           </styleClass>
                        </Button>
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
         </content>
    </Tab>
      <Tab text="Liedjes">
         <content>
            <HBox focusTraversable="true">
               <children>
                  <ListView focusTraversable="false" prefHeight="237.0" HBox.hgrow="ALWAYS" />
                  <VBox alignment="TOP_CENTER" spacing="10.0" style="-fx-border-color: #C8C8C8; -fx-border-width: 1px;" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <padding>
                        <Insets top="5.0" />
                     </padding>
                     <children>
                        <Button focusTraversable="false" mnemonicParsing="false" prefHeight="0.0" prefWidth="30.0" text="+">
                           <styleClass>
                              <String fx:value="btn" />
                              <String fx:value="btn-toolIcon" />
                              <String fx:value="text-green" />
                           </styleClass>
                        </Button>
                        <Button focusTraversable="false" mnemonicParsing="false" text="-">
                           <styleClass>
                              <String fx:value="btn" />
                              <String fx:value="btn-toolIcon" />
                              <String fx:value="text-red" />
                           </styleClass>
                        </Button>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </content>
      </Tab>
  </tabs>
</TabPane>

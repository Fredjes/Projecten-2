<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="650.0" prefWidth="1000.0" styleClass="white" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.45" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" styleClass="centerGlow" vgap="10.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane alignment="CENTER" hgap="10.0" vgap="10.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" />
                  <ColumnConstraints hgrow="SOMETIMES" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints vgrow="SOMETIMES" />
                  <RowConstraints vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <VBox fx:id="gebruikersBeheren" alignment="CENTER" onMouseClicked="#onManageUsers" prefHeight="200.0" prefWidth="200.0" spacing="10.0" GridPane.rowIndex="1">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="G">
                           <styleClass>
                              <String fx:value="icon" />
                              <String fx:value="icon-gebruikers-beheren" />
                           </styleClass></Text>
                        <Label fx:id="lblGebruikers" text="Gebruikers beheren" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <styleClass>
                        <String fx:value="user-management-tile" />
                        <String fx:value="tile" />
                     </styleClass>
                  </VBox>
                  <VBox fx:id="excelImporteren" alignment="CENTER" onMouseClicked="#onImportExcel" prefHeight="200.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="I">
                           <styleClass>
                              <String fx:value="icon" />
                              <String fx:value="icon-excel-importeren" />
                           </styleClass></Text>
                        <Label fx:id="lblExcel" text="Excel importeren" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <styleClass>
                        <String fx:value="import-excel-tile" />
                        <String fx:value="tile" />
                     </styleClass>
                  </VBox>
                  <VBox fx:id="UitleningenBeheren" alignment="CENTER" onMouseClicked="#onManageLoans" prefHeight="200.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1">
                     <children>
                        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="U">
                           <styleClass>
                              <String fx:value="icon" />
                              <String fx:value="icon-uitleningen-beheren" />
                           </styleClass></Text>
                        <Label fx:id="lblUitleningen" text="Uitleningen beheren" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <styleClass>
                        <String fx:value="loan-management-tile" />
                        <String fx:value="tile" />
                     </styleClass>
                  </VBox>
                  <VBox fx:id="voorwerpenBeheren" alignment="CENTER" onMouseClicked="#onManageItems" prefHeight="200.0" prefWidth="200.0" spacing="10.0">
                     <children>
                        <Text fx:id="voorwerpenBeherenIcon" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="V">
                           <styleClass>
                              <String fx:value="icon" />
                              <String fx:value="icon-voorwerpen-beheren" />
                           </styleClass></Text>
                        <Label fx:id="lblVoorwerpen" text="Voorwerpen beheren" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <styleClass>
                        <String fx:value="item-management-tile" />
                        <String fx:value="tile" />
                     </styleClass>
                  </VBox>
               </children>
            </GridPane>
         </children>
      </FlowPane>
   </center>
</fx:root>

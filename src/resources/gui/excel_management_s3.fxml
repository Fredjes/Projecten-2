<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root styleClass="white" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox alignment="CENTER_LEFT" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Text fx:id="title" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="bold" text="Geïmporteerd" />
            <Label text="Alle Excel-bladen zijn geïmporteerd en zijn toegankelijk over heel de applicatie. U kan terugkeren naar het hoofdmenu of de applicatie afsluiten." wrapText="true" />
            <Separator maxWidth="1.7976931348623157E308" />
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </VBox>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="50.0" spacing="8.0" BorderPane.alignment="CENTER">
         <children>
            <Separator HBox.hgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <VBox alignment="CENTER" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Proficiat, alles is geïmporteerd!">
               <font>
                  <Font size="18.0" />
               </font>
            </Text>
            <Button mnemonicParsing="false" onAction="#onMainScreen" text="Terugkeren naar het hoofdmenu">
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-lime" />
               </styleClass>
            </Button>
         </children>
      </VBox>
   </center>
</fx:root>

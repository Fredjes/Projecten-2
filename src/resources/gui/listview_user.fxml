<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" />
    <ColumnConstraints hgrow="SOMETIMES" />
      <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints valignment="BOTTOM" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Text fx:id="classroom" strokeType="OUTSIDE" strokeWidth="0.0" text="Klas" GridPane.columnIndex="2">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Text fx:id="email" strokeType="OUTSIDE" strokeWidth="0.0" text="E-mailadres" GridPane.columnIndex="1">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Text fx:id="name" strokeType="OUTSIDE" strokeWidth="0.0" text="Naam">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>

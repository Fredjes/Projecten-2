<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root hgap="10.0" type="GridPane" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" />
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Button mnemonicParsing="false" onAction="#onChangeUser" text="Verander gebruiker">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-lime" />
         </styleClass>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#onChangeItemCopy" text="Verander exemplaar" GridPane.columnIndex="1">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="btn-lime" />
         </styleClass>
      </Button>
      <Label text="Startdatum:" GridPane.rowIndex="1" />
      <Label text="Einddatum:" GridPane.rowIndex="2" />
      <DatePicker fx:id="startDate" prefWidth="156.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <DatePicker fx:id="endDate" prefWidth="156.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
